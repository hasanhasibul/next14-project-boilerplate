{
	"extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
	"rules": {
		"no-unused-vars": "error",
		"prefer-const": "error",
		"no-unused-expressions": "error",
		"no-undef": "error",
		"no-console": "warn"
		// "@typescript-eslint/consistent-type-definitions": ["error", "type"]
	},

	"env": {
		"browser": true,
		"es2021": true
	}
}
